{% with rand = code|dateHash %}
<div class="em-drop" data-toggle="clps" data-target="#git_code_{{ rand }}">View Runtime Code<svg class="em-dropIcon" viewBox="0 0 320 512" xmlns="http://www.w3.org/2000/svg"><path d="m143 352.3-136-136c-9.4-9.4-9.4-24.6 0-33.9l22.6-22.6c9.4-9.4 24.6-9.4 33.9 0l96.4 96.4 96.4-96.4c9.4-9.4 24.6-9.4 33.9 0l22.6 22.6c9.4 9.4 9.4 24.6 0 33.9l-136 136c-9.2 9.4-24.4 9.4-33.8 0z"/></svg></div>

<div class="clps" id="git_code_{{ rand }}">
    <div class="em-dropBody">

        <div class="em-tableTools">
            <div>
                <button class="" onclick="copyQuery{{ rand }}(event)" title="copy"><svg viewBox="0 0 448 512" xmlns="http://www.w3.org/2000/svg"><path d="M320 448v40c0 13.255-10.745 24-24 24H24c-13.255 0-24-10.745-24-24V120c0-13.255 10.745-24 24-24h72v296c0 30.879 25.121 56 56 56h168zm0-344V0H152c-13.255 0-24 10.745-24 24v368c0 13.255 10.745 24 24 24h272c13.255 0 24-10.745 24-24V128H344c-13.2 0-24-10.8-24-24zm120.971-31.029L375.029 7.029A24 24 0 0 0 358.059 0H352v96h96v-6.059a24 24 0 0 0-7.029-16.97z"></path></svg></button>
                <button class="" onclick="saveQuery{{ rand }}(event)" title="download"><svg viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg"><path d="M216 0h80c13.3 0 24 10.7 24 24v168h87.7c17.8 0 26.7 21.5 14.1 34.1L269.7 378.3c-7.5 7.5-19.8 7.5-27.3 0L90.1 226.1c-12.6-12.6-3.7-34.1 14.1-34.1H192V24c0-13.3 10.7-24 24-24zm296 376v112c0 13.3-10.7 24-24 24H24c-13.3 0-24-10.7-24-24V376c0-13.3 10.7-24 24-24h146.7l49 49c20.1 20.1 52.5 20.1 72.6 0l49-49H488c13.3 0 24 10.7 24 24zm-124 88c0-11-9-20-20-20s-20 9-20 20 9 20 20 20 20-9 20-20zm64 0c0-11-9-20-20-20s-20 9-20 20 9 20 20 20 20-9 20-20z"></path></svg></button>
            </div>
        </div>


        <div class="em-codeBox" style="height:500px;">
            <div ss-container class="ss-preview light" style="height:100%"><pre><code class="language-sql">{{ code }}</code></pre></div>
        </div>
        <script>
            var readyStateCheckInterval{{ rand }} = setInterval(function() {
                if (document.readyState === "complete") {
                    clearInterval(readyStateCheckInterval{{ rand }});

                    SimpleScrollbar.initAll();Prism.highlightAll();

                    window.copyQuery{{ rand }} = function(e) {
                        var code = document.getElementById('git_code_{{ rand }}').querySelector('.em-codeBox pre').innerText;
                        var txt = document.createElement('textarea');
                        txt.value = code;
                        txt.setAttribute('readonly', '');
                        txt.style = {
                          position: 'absolute',
                          left: '-9999px'
                        };
                        document.body.appendChild(txt);
                        txt.select();
                        document.execCommand('copy');
                        document.body.removeChild(txt);
                      };
                    window.saveQuery{{ rand }} = function(e) {
                        var a = document.createElement("a");
                        a.href = URL.createObjectURL(new Blob([document.getElementById('git_code_{{ rand }}').querySelector('.em-codeBox pre').innerText], {
                            type: "text/sql"
                        }));
                        a.setAttribute("download", "query.sql");
                        document.body.appendChild(a);
                        a.click();
                        document.body.removeChild(a);
                      };                   
                }
            }, 10);             
        </script>
    </div>
</div>
{% endwith %}